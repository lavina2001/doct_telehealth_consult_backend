			<div class="content">
				<div class="container-fluid">

					<div class="row">
						<div class="col-md-12 col-lg-4 col-xl-3 theiaStickySidebar">
						
							
						</div>
						
						<div class="col-md-12 col-lg-8 col-xl-9">

							
                            {{#each data}}
							<div class="card">
								<div class="card-body">
									<div class="doctor-widget">
										<div class="doc-info-left">
											<div class="doctor-img">
												<a href="/doctorProfile?did={{this.doctor._id}}">
													<img src="img/dp.png" class="img-fluid" alt="">
												</a>
											</div>
											<div class="doc-info-cont">
												<h4 class="doc-name"><a href="/doctorProfile">{{this.doctor.name}} </a></h4>
												<p class="doc-speciality">{{this.doctor.qualification}}</p>
												<h5 class="doc-department"><img src={{this.speciality.picture}} class="img-fluid" alt="Speciality">{{this.speciality.title}}</h5>
												<div class="rating">
													<i class="fas fa-star filled"></i>
													<i class="fas fa-star filled"></i>
													<i class="fas fa-star filled"></i>
													<i class="fas fa-star filled"></i>
													<i class="fas fa-star"></i>
													<span class="d-inline-block average-rating">(17)</span>
												</div>
																							</div>
										</div>
										
									</div>
								</div>
							</div>
                            {{/each}}

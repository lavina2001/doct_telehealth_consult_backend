<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FeedBack</title>
</head>

<body>
    <center>
        <h1>Feedback</h1>
    </center>






    <div class="row">
        <div class="col-md-12">

            <!-- Recent Orders -->
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="datatable table table-hover table-center mb-0">
                            <thead>
                                <tr>
                                    <th>Doctor Name</th>
                                    <th>FeedBack</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each this.response}}
                                <tr>
                                    <td>
                                        {{this.name}}
                                    </td>
                                    <td>
                                        {{#each this.feedback}}
                                        {{this.feedback}}
                                        {{/each}}
                                    </td>
                                    {{!-- <td>neurologist</td> --}}
                                </tr>
                                {{/each}}
                                <!-- /Recent Orders -->

                    </div>
                </div>
            </div>
        </div>
        <!-- /Page Wrapper -->

    </div>


</body>


</html>